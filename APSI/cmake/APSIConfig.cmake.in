# Exports target APIS::api
#
# Creates variables:
#   APSI_BUILD_TYPE : The build configuration used
#   APSI_DEBUG : Set to non-zero value if APSI is compiled with extra debugging code

set(APSI_BUILD_TYPE @CMAKE_BUILD_TYPE@)
set(APSI_DEBUG @APSI_DEBUG@)

include(${CMAKE_CURRENT_LIST_DIR}/APSITargets.cmake)
message(STATUS "APSI detected (version ${APSI_VERSION})")
message(STATUS "APSI build type: ${APSI_BUILD_TYPE}")
message(STATUS "APSI debug mode: ${APSI_DEBUG}")
