# Exports target APSISender::apsi_sender
#
# Creates variables:
#   APSI_BUILD_TYPE : The build configuration used
#   APSI_DEBUG : Set to non-zero value if APSI is compiled with extra debugging code

set(APSI_BUILD_TYPE @CMAKE_BUILD_TYPE@)
set(APSI_DEBUG @APSI_DEBUG@)

include(${CMAKE_CURRENT_LIST_DIR}/APSISenderTargets.cmake)
message(STATUS "APSISender detected (version ${APSISender_VERSION})")
message(STATUS "APSISender build type: ${APSI_BUILD_TYPE}")
message(STATUS "APSISender debug mode: ${APSI_DEBUG}")
