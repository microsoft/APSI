# Exports target APSI::Receiver

@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

macro(apsi_find_dependency dep)
    find_dependency(${dep})
    if(NOT ${dep}_FOUND)
        if(NOT APSIReceiver_FIND_QUIETLY)
            message(WARNING "Could not find dependency `${dep}`")
        endif()
        set(APSIReceiver_FOUND FALSE)
        return()
    endif()
endmacro()

set(APSIReceiver_FOUND FALSE)

# Locate APSICommon dependency
apsi_find_dependency(APSICommon 1.0.0 EXACT REQUIRED)

include(${CMAKE_CURRENT_LIST_DIR}/APSIReceiverTargets.cmake)

if(TARGET APSI::Receiver)
    if(NOT APSIReceiver_FIND_QUIETLY)
        message(WARNING "APSIReceiver -> Version ${APSIReceiver_VERSION} detected")
    endif()
    set(APSIReceiver_FOUND TRUE)
else()
    if(NOT APSIReceiver_FIND_QUIETLY)
        message(WARNING "APSIReceiver -> NOT FOUND")
    endif()
    set(APSIReceiver_FOUND FALSE)
endif()
